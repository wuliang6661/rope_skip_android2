                        android:layout_height="20dp"
                        android:background="@mipmap/icon_mk2" />

                    <android.support.v7.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp"
