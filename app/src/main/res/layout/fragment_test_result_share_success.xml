                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="14dp"
                    android:text="平均速度"
                    android:textColor="#ffb9cae0"
                    android:textSize="12sp" />

